{
  "name": "xbar-internet-speedtest",
  "version": "0.0.0",
  "description": "xbar plugin to test you internet speed in regular interval and display upload and download speed in the menu bar",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "repository": "chrisspiegl/xbar-internet-speedtest",
  "author": {
    "name": "Chris Spiegl",
    "email": "chris@chrisspiegl.com",
    "url": "https://chrisspiegl.com/"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "cu": "ncu",
    "cu:fix": "ncu -u",
    "test": "xo --fix"
  },
  "files": [
    "icon.png",
    "index.js",
    "index.sh"
  ],
  "keywords": [
    "xbar",
    "plugin",
    "download",
    "upload",
